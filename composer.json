{
    "name": "jacques/plutus-promocodes",
    "description": "Promotional Codes for Plutus",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.3 || ^7.4",
        "illuminate/database": "^8.6",
        "illuminate/pagination": "^8.6"
    },
    "require-dev": {
        "brick/varexporter": "^0.3.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpro/grumphp": "^0.18",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.3",
        "rector/rector": "^0.8.10",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.16"
    },
    "autoload": {
        "psr-4": {
            "Plutus\\Plugins\\Promocodes\\": "src/"
